    "url": "git+https://github.com/j-/gamepad.git"
  },
  "keywords": [
    "gamepad",
    "controller",
    "xbox",
    "360",
    "xbone",
    "playstation",
    "ps3",
    "ps4",
    "joystick"
  ],
  "author": "Jamie Hoeks <j@skeoh.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/j-/gamepad/issues"
  },
  "homepage": "https://github.com/j-/gamepad#readme",
  "devDependencies": {
    "ava": "^0.15.2",
    "babel": "^6.5.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.11.0",
    "babel-register": "^6.9.0",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "webpack": "^1.13.1"
  },
  "ava": {
    "files": [
      "**/*.test.js"
    ],
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "babel": {
    "plugins": [
